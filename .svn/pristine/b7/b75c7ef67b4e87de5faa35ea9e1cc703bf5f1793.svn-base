        $(".z1_huang").each(function () {
            var a = $(this).text();
            var b = [];
            var jiashu = 0;
            var c="";
            while (a > 0) {
                if (jiashu % 3 == 0 && jiashu!=0)
                    b.push(",");
                b.push(a % 10);
                jiashu++;
                a = parseInt(a / 10);
            }
            c=b.reverse().join("");
            $(this).text(c);
         
        })


        $(".toup_2_a").click(function () {
            if ($(this).text() == "为他投票") {
