body, ol, ul, h1, h2, h3, h4, h5, h6, p, th, td, dl, dd, form, fieldset, legend, input, textarea, select {
    margin: 0;
    padding: 0;
}

body {
    font: 12px"宋体","Arial Narrow",HELVETICA;
    background: #fff;
    -webkit-text-size-adjust: 100%;
}

a {
    color: #2d374b;
    text-decoration: none;
}

    a:hover {
        color: #cd0200;
        text-decoration: underline;
    }

em {
    font-style: normal;
}

li {
    list-style: none;
}

img {
    border: 0;
    vertical-align: middle;
}

table {
    border-collapse: collapse;
    border-spacing: 0;
}

span, b {
    display: inline-block;
}

p {
    word-wrap: break-word;
}

.p10 {
    padding: 10px !important;
}

.p0 {
    padding: 0px !important;
}

.shengl {
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.right {
    float: right !important;
}

.left {
    float: left;
}

.tu_index {
    position: relative;
    padding: 0px;
}

    .tu_index img {
        width: 100%;
    }

.kanj_nr_bj {
    background: url(../images/kanj_nriy.png) repeat-y;
    background-size: 100% auto;
}

.kanj_kaishi {
    padding: 10px 0px;
    padding-bottom: 70px;
    box-sizing: content-box;
}

    .kanj_kaishi > img {
        width: 55%;
    }

    .kanj_kaishi > * {
        margin-bottom: 15px;
    }

    .kanj_kaishi > .kanj1_p {
        color: #B39330;
        font-size: 1.4rem;
    }

.kanj_btn > img {
    width: 70%;
}

.mo_se {
