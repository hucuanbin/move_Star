/* 分享弹窗*/
.huod_tc {
  position: relative;
  padding: 0px;
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
}
.huod_tc > img {
  width: 100%;
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
}
.huod_tc > a {
  font-size: 6vw;
  color: #fff;
  position: absolute;
  top: 10px;
  right: 5px;
  padding-left: 20px;
  padding-bottom: 20px;
}
.huod_tc_btn {
  text-align: center;
  background: #191919;
  border: none;
}
.huod_tc_btn > button {
  width: 40%;
  height: 40px;
  background: url(../images/huod_fenx_btn.png) no-repeat;
  background-size: 100% 100%;
  border: none;
  outline: none;
}
