.loading {
    position: absolute;
    width: 60%;
    left: 50%;
    margin-left: -30%;
    top: 104px;
    background: #1E1E1E;
    height: 62px;
    overflow: hidden;
}

    .loading ul {
        overflow: auto;
        margin-top: 25.6px;
        padding: 0px 10px;
    }

    .loading li {
        width: 12.5%;
        text-align: center;
        float: left;
    }

    .loading span {
        display: inline-block;
        height: 10px;
        width: 10px;
        border-radius: 5PX;
        background: #fff;
    }

    .loading > b {
        position: absolute;
        display: inline-block;
        width: 64px;
        left: -20%;
        height: 64px;
        background: -moz-linear-gradient(left, blue, white);
        background: linear-gradient(left, blue, white);
        background: -webkit-linear-gradient(left, rgba(30,30,30,0.0),rgba(30,30,30,5),rgba(30,30,30,1), rgba(30,30,30,1),rgba(30,30,30,0.5),rgba(30,30,30,0.0));
        animation: jiantou_left 3s infinite;
        -webkit-animation: jiantou_left 3s infinite; /* Safari 和 Chrome */
    }

@keyframes jiantou_left {
    0% {
        left: -20%;
    }

    100% {
        left: 100%;
    }
}

@-moz-keyframes jiantou_left /* Firefox */
{
    0% {
        left: -20%;
    }

    100% {
        left: 100%;
    }
}

@-webkit-keyframes jiantou_left /* Safari 和 Chrome */
{
    0% {
        left: -20%;
    }

    100% {
        left: 100%;
    }
}

@-o-keyframes jiantou_left /* Opera */
{
    0% {
        left: -20%;
    }

    100% {
        left: 100%;
    }
}

