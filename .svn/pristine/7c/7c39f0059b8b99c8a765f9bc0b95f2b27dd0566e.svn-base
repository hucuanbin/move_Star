    <script>
        $(document).ready(function () {
            $(".YYY_pd_zong>img").each(function () {
                var a = $(this).index();
                $(this).css({
                    "top": a * 70 + 50,
                })
                $(this).addClass("yb");
            })

            var zong_cishu;
            var biaoji = 1;
            var bbshi = 1;
            var c = setInterval(
                 function () {
                     $(".YYY_pd_zong>img").attr("data-bl", 0);
                     $.ajax({
                         url: 'aaa.txt?' + Math.random(),
                         dataType: "json",
                         success: function (data) {
                             for (var i = 0; i < data.length; i++) {
                                 zong_cishu = data[i].zong_cishu / 2.5;
                                 var now_cishu;
                                 var biaobiaoJI;

                                 if (zong_cishu <= data[i].cishu) {
                                     $(".YYY_pd_zong").children("img").each(function () {
                                         var my_id = $(this).attr("id");
                                         var i_dd = data[i].id;
                                         if (my_id == i_dd) {
                                             now_cishu = $(this).index();
                                             biaobiaoJI = now_cishu;
                                             $(".YYY_pd_zong>img:eq(" + now_cishu + ")").removeClass("yb");
                                             $(".YYY_pd_zong>img:eq(" + now_cishu + ")").css({
                                                 "left": 82 + "%"
                                             })
                                         }
                                     })
                                     var b = '<img src="images/YYY_paodao_wei1.png" class="pd_gan left" /> '
                                     var a = "<span class='xingm'>小强强强强强</span>"
                                     //以上小强强强应该为变量  用户名
                                     $(".YYY_pd_zong>div:eq(" + biaobiaoJI + ")>div:last").html(b + a);
                                     if (biaobiaoJI + 1 == data.length) {
                                         $(".zheme").show();
                                     }                                  
                                 }
                                 else {
                                     var i_d = data[i].id;
                                     var a = data[i].mingci;
                                     bbshi = 1;
                                     var b = data[i].cishu * 6.8 + 15 - (a - 1) * 2;
                                     $(".YYY_pd_zong").children("img").each(function () {
                                         var my_id = $(this).attr("id");
                                         var qian_mingci = $(this).attr("data-mingci");
                                         if (my_id == i_d) {
                                             var now_cishu = $(this).index();
                                             $(this).attr("data-mingci", a);
                                             $(".YYY_pd_zong>img:eq(" + now_cishu + ")").css({
                                                 "top": (a - 1) * 70 + 50,
                                                 "left": b + "%"
                                             })
                                             $(this).attr("src", data[i].src);
                                             $(this).attr("data-qian", qian_mingci);
                                             $(this).attr("data-bl", 1);
                                             bbshi = 0;
                                             return;
                                         }
                                     })
                                     if (bbshi == 1) {
                                         $(".YYY_pd_zong").children("img").each(function () {
                                             if ($(this).attr("data-bl") == 0);
                                                    $(this).remove();
                                         })

                                     }
                                 }
                             }
                         }
                     });


                 }, 2500);
            $(".YYY_pd_zong>img").attr("data-qian", -1);


        })

    </script>
