.loding_wai {
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.7);
    position: fixed;
    top: 0px;
    left: 0px;
    z-index: 999;
}

.loading {
    position: absolute;
    width: 60%;
    left: 50%;
    margin-left: -30%;
    top: 104px;
    background: #1E1E1E;
    height: 62px;
    overflow: hidden;
}

    .loading span {
        display: inline-block;
        height: 10px;
        width: 10px;
        border-radius: 5PX;
        background: #fff;
    }

    .loading > b {
        position: absolute;
        display: inline-block;
        width: 64px;
        left: -20%;
        height: 64px;
        background: -moz-linear-gradient(left, blue, white);
        background: linear-gradient(left, blue, white);
        background: -webkit-linear-gradient(left, rgba(30,30,30,0.0),rgba(30,30,30,5),rgba(30,30,30,1), rgba(30,30,30,1),rgba(30,30,30,0.5),rgba(30,30,30,0.0));
        animation: jiantou_left 3s infinite;
        -webkit-animation: jiantou_left 3s infinite;
    }

    .loading ul {
        overflow: auto;
        margin-top: 25.6px;
        padding: 0px 10px;
    }

    .loading li {
        width: 12.5%;
        text-align: center;
        float: left;
    }

@keyframes jiantou_left {
    0% {
        left: -20%;
    }

    100% {
        left: 100%;
    }
}

@-moz-keyframes jiantou_left /* Firefox */
{
    0% {
        left: -20%;
    }

    100% {
        left: 100%;
    }
}

@-webkit-keyframes jiantou_left /* Safari 和 Chrome */
{
    0% {
        left: -20%;
    }

    100% {
        left: 100%;
    }
}

@-o-keyframes jiantou_left /* Opera */
{
    0% {
        left: -20%;
    }

    100% {
        left: 100%;
    }
}

